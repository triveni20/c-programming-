#include <stdio.h>
int main()
{
    int a,b,c,d,e;
    scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
    if(a>b && a>c)
    {
        if(a>d && a>e)
        {
            printf(" a is largest ");
        }
    }
    if(b>c && b>d)
    {
        if(b>e && b>a)
        {
            printf(" b is largest ");
        }
    }
    if(c>d && c>e)
    {
        if(c>a && c>e)
        {
            printf(" c is largest ");
        }
    }
    if(d>e && d>a)
    {
        if(d>b && d>b)
        {
            printf(" d is largest ");
        }
    }
    if(e>a && e>b)
    {
        if(e>c && e>d)
        {
            printf(" e is largest ");
        }
    }
}
Out put1:- 1 2 3 4 5   e is largest
Out put 2:- 1 2 3 5 4     d is largest
Out put 3:- 5 3  4 3 2   a is largest
Out put 4:- 3 2 4 1 0    c is largest
Out put 5:- 3 2 4 1 0    c is largest
Out put 6:- -1 -2 0 -2  -2   c is largest
Out put 7:- 2 3 0 -2 -3    b is largest
Out put 8:- 1 1 1 3 3  (didn’t get any output)
Out put 9:- 1 1 1 1 2    e is largest
Out put 10:- 2 2 3 3 1  d is largest
Out put 11:- 3 3 2 3 3(didn’t get any output)
Out put 12:- 1  1  1 1 1(didn’t get any output)
Out put 13:- 1 1 1 2 1  d is largest

Program 2:-
#include <stdio.h>
int main ()
{
    int a,b,c,d,e;
    
    scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
   if(a>b)
   {
       if(a>c)
       {
           if(a>d && a>e)
           {
               printf(" largest is a");
           }
       }
   }
   if(b>a && b>c)  
   {
       if(b>d)
       {
           if(b>e)
           {
               printf("b is largest");
           }
       }
   }
 if (c>a && c>b)
   {
       if (c>d)
       {
           if(c>e)
           {
               printf("c is largest");
           }
       }
   }
 if (d>a && d>b)
   {
       if (d>c)
       {
           if(d>e)
           {
               printf("d is largest");
           }
            else
            {
                printf(" e is largest");
            }
   
       }
   }
   
}
out put:-
Out put 8:- 1 1 1 3 3  e is largest 



