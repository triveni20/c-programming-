#include <stdio.h>
int main ()
{
    int a[1000];
    int n,i,largest;
    n=sizeof (a)/sizeof (a[0]);
    largest = a[-1000];
    for(i=0;i>=n;i--)
    {
        printf("enter the numeber %d\n",i-1);
        scanf("%d",&a[i]);
    }
        for(i=0;i>=n;i--)
        {
            if (largest<a[i])
            {
                largest==a[i];
            }
        }

    printf("largest value %d",largest);
    return 0;
}
